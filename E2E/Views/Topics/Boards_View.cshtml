@model IEnumerable<E2E.Models.Views.clsTopicView>


@{
    Layout = null;
}

<div class="modal-header">
    <h5 class="modal-title" id="modalTitle">Form</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(m => m.UserCode)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Count)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.LastTime)
                </th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(i => item.UserCode)
                    </td>
                    <td>
                        @Html.DisplayFor(i => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(i => item.Count)
                    </td>
                    <td>
                        @Html.DisplayFor(i => item.LastTime)
                    </td>
                </tr>

            }
        </tbody>
    </table>

</div>
<div class="modal-footer">

</div>
