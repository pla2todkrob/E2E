@model E2E.Models.Tables.Topics


@{
    ViewBag.Title = "Boards_Form";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Boards_Form</h2>

<div class="form-row">
    <div class="form-group col-md-12">
        <asp:Label runat="server" Text=""></asp:Label>
        @Html.DisplayFor(m => m.Topic_Title, new { @class = "form-control" })
    </div>

    <div class="form-group col-md-12">
        @Html.DisplayFor(m => m.Topic_Content, new { @class = "form-control" })
    </div>
</div>


<div class="mr-auto">
    <button class="btn btn-secondary" onclick="location.href='@Url.Action("Boards", "Topics")'"><i class="fa fa-chevron-left"></i>&nbsp;</button>
</div>


