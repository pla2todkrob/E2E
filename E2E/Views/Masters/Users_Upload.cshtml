@{
    Layout = null;
}


<div class="modal-header">
    <h5 class="modal-title" id="modalTitle">Import</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    @using (Html.BeginForm("", "", FormMethod.Post, new { id = "UploadForm", name = "UploadForm", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(false, "", new { @class = "text-danger" })
        <div class="form-group">
            <label for="importFile">File</label>
            <input name="importFile" type="file" class="form-control-file" id="importFile" required />
        </div>
    }
    <hr />

    @{ Html.RenderAction("UploadHistory", "Users"); }
</div>
<div class="modal-footer">
    <button type="submit" class="btn btn-primary" form="UploadForm"><i class="fa fa-check" aria-hidden="true"></i></button>
</div>

