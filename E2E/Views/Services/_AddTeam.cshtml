@model E2E.Models.Views.ClsServiceTeams

@{
    Layout = null;
}

@using (Html.BeginForm("", "", FormMethod.Post, new { id = "TeamForm", @class = "was-validated" }))
{
    @Html.ValidationSummary(true)
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.Service_Id)
    <div class="form-row">
        <div class="form-group col-9">
            @Html.DropDownListFor(m => m.User_Ids, new SelectList(ViewBag.TeamList, "Value", "Text"), new { @class = "form-control", required = "required", multiple = "multiple" })
        </div>
        <div class="form-group col-3">
            <button type="submit" class="btn btn-primary">Add team</button>
        </div>
    </div>

}
