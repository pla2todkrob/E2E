@model IEnumerable<E2E.Models.Tables.ChatGPT> 


@{
	ViewBag.Title = "Usage_Detail";
	Layout = null;
}


<div class="modal-header">
	<h5 class="modal-title">Detail</h5>
	<button type="button" class="close" data-dismiss="modal" aria-label="Close">
		<span aria-hidden="true">&times;</span>
	</button>
</div>
<div class="modal-body">
	<div class="row">
		<table class="display table table-bordered table-hover w-100">
			<thead>
				<tr>
					<th class="fit">@Html.DisplayNameFor(m => m.FirstOrDefault().Create)</th>
					<th class="fit">@Html.DisplayNameFor(m => m.FirstOrDefault().Question)</th>
					<th>@Html.DisplayNameFor(m => m.FirstOrDefault().Answer)</th>
					<th class="fit">@Html.DisplayNameFor(m => m.FirstOrDefault().Tokens)</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var item in Model)
				{

				<tr>
					<td class="fit">@Html.DisplayFor(i => item.Create)</td>
					<td class="fit">@Html.DisplayFor(i => item.Question)</td>
					<td>@Html.DisplayFor(i => item.Answer)</td>
					<td class="fit">@Html.DisplayFor(i => item.Tokens)</td>
				</tr>

				}
			</tbody>
		</table>
	</div>
</div>