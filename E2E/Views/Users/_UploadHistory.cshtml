@model IEnumerable<E2E.Models.Tables.UserUploadHistories>

@{
    Layout = null;
}

@{
    if (Model.Count() > 0)
    {
        <h5>Upload history</h5>
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(m => m.Create)</th>
                    <th>@Html.DisplayNameFor(m => m.UserUploadHistoryFileName)</th>
                    <th>@Html.DisplayNameFor(m => m.Users.User_Code)</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(m => item.Create)</td>
                        <td><a href="@Html.ValueFor(m => item.UserUploadHistoryFile)" download="" target="_blank">@Html.DisplayFor(m => item.UserUploadHistoryFileName)</a></td>
                        <td>@Html.DisplayFor(m => item.Users.User_Code)</td>
                    </tr>
                }
            </tbody>
        </table>
    }
}