@model IEnumerable<E2E.Models.Tables.ChatBotQuestion>

@{
    Layout = null;
}

<div class="list-group">
    @{
        if (Model.Count() == 0)
        {
            if (ViewBag.ChatbotGroupList != null)
            {
                foreach (var item in ViewBag.ChatbotGroupList)
                {
        <a href="javascript:reloadTable({id:'@item.Value'})" class="list-group-item list-group-item-action">@item.Text</a>
                }
            }

        }
        else
        {
            foreach (var item in Model)
            {
        <a href="javascript:reloadTable({id:'@item.ChatBotQuestion_Id'})" class="list-group-item list-group-item-action">@item.Question</a>
            }
        }
    }
</div>
